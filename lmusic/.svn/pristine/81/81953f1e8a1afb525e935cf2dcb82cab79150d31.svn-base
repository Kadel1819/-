<script type="text/javascript">
	$(".img-slide").click(ChangeImg);
    var index=0;
    //改变图片
    function ChangeImg() {
        index++;
        var a=document.getElementsByClassName("img-slide");
        if(index>=a.length) index=0;
        for(var i=0;i<a.length;i++){
            a[i].style.display='none';
        }
        a[index].style.display='block';
    }
    //设置定时器，每隔两秒切换一张图片
    setInterval(ChangeImg,3000);
    /* 头像事件 */
    $("#img-header").mouseover(function(){
    	$("#diy-header").css({"display":"block"});
    });
    $("#diy-header").click(function(){
    	$("#diy-header").css({"display":"none"});
    });
    /* 显示用户头像start */
    $(document).ready(function(){
			//尝试获取存入的头像路径值
			let cookieAvatar = $.cookie("avatar");
 			console.log("cookieAvatar"+cookieAvatar);
 			//尝试显示头像
 			if(cookieAvatar != null){
 				$("#img-header").attr("src",cookieAvatar);
 			}
	});
    /* 显示用户头像end */
    
    /* 分页jq代码 */
    var div;
    $(".paging-n>div").click(function(){
    	div=$(this).css({"background-color":"#ffe443"});/* 颜色 */
    	$(this).siblings().css("background-color", "#FFFAFA");
    });
    $(".paging-n>div").mouseover(function(){
    	$(this).css({"background-color":"#ffe443"});/* 颜色 */
    	$(this).siblings().css("background-color", "#FFFAFA");
    	$(div).css({"background-color":"#ffe443"});/* 颜色 */
    });
    $(".paging-n>div").mouseout(function(){
    	$(this).css({"background-color":"#FFFAFA"});
    	$(div).css({"background-color":"#ffe443"});/* 颜色 */
    });
    /* 分页 */
    /* 退出登录 */
     function exit(){
    	console.log('准备退出登录');
    	$.ajax({
			"url":"/users/exit",
			"type":"get",
			"dataType":"json",
			"success":function(result){
				if(result.state == 2000){
					alert("退出登录成功!!!");
					location.href="index.html";/* 如果注册成功就跳转登录页面 */
				}
			 },
		});
    }
</script>
